{
	"$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
	"packageManager": "npm",
	"reporters": [
		"html",
		"clear-text",
		"progress"
	],
	"testRunner": "jest",
	"coverageAnalysis": "perTest",
	"mutate": [
		"src/**/*.ts",
		"!src/main.ts",
		"!src/**/*.spec.ts",
		"!src/**/*.module.ts"
	],
	"jest": {
		"configFile": "package.json",
		"enableFindRelatedTests": false
	}
}
